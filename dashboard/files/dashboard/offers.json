      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  FROM_UNIXTIME(U.date, '%d/%m/%Y %H:%i') as 'DATE & TIME',\n  U.url as URL,\n  O.name as NAME,\n  O.modelDate as YEAR,\n  O.price as PRICE,\n  S.score as SCORE,\n  O.numberOfDoors as DOORS,\n  O.mileageFromOdometer as ODOMETER,\n  O.vehicleTransmission as TRANSMISSION,\n  O.fuelType as FUEL,\n  O.carColor as COLOR\nFROM \n  offer O\nINNER JOIN score S ON O.code = S.code \nINNER JOIN url U ON O.code = U.code\nWHERE \n  O.state in ($Region)\nAND \n  O.carColor in ($Color)\nAND\n  O.brand in ($Brand)\nAND \n  FROM_UNIXTIME(U.date) >= $__timeFrom()\nAND\n  FROM_UNIXTIME(U.date) <= $__timeTo()\nORDER BY\n  U.date DESC;",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
